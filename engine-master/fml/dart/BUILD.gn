# Utilities for working with FML types with the Dart API. Targets that use FML
# as well as Dart must explicitly include this target as FML itself cannot
# depend on Dart.
source_set("dart") {
  sources = [
    "dart_converter.cc",
    "dart_converter.h",
  ]

  public_deps = [
    "$flutter_root/fml",
    "$flutter_root/runtime:libdart",
    "//third_party/tonic",
  ]
}
